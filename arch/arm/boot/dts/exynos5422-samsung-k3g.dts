/* eMMC flash */
&mmc_0 {
	status = "okay";
	mmc-hs200-1_8v;
	cap-mmc-highspeed;
	non-removable;
	clock-frequency = <400000000>;
	samsung,dw-mshc-ciu-div = <3>;
	samsung,dw-mshc-sdr-timing = <0 4>;
	samsung,dw-mshc-ddr-timing = <0 2>;
	samsung,dw-mshc-hs400-timing = <0 2>;
	samsung,read-strobe-delay = <90>;
	pinctrl-names = "default";
	pinctrl-0 = <&sd0_clk &sd0_cmd &sd0_bus1 &sd0_bus4 &sd0_bus8 &sd0_rclk>;
	bus-width = <8>;
};

/* WiFi SDIO module */
&mmc_1 {
	status = "disabled";
	non-removable;
	cap-sdio-irq;
	keep-power-in-suspend;
	clock-frequency = <400000000>;
	samsung,dw-mshc-ciu-div = <3>;
	samsung,dw-mshc-sdr-timing = <2 3>;
	samsung,dw-mshc-ddr-timing = <1 2>;
	pinctrl-names = "default";
	pinctrl-0 = <&sd1_clk>, <&sd1_cmd>, <&sd1_int>, <&sd1_bus1>,
			<&sd1_bus4>, <&sd1_bus8>, <&wifi_en>;
	bus-width = <4>;
	cap-sd-highspeed;
	vqmmc-supply = <&buck10_reg>;
	mmc-pwrseq = <&mmc1_pwrseq>;

	wifi@1 {
		compatible = "brcm,bcm4354-fmac", "brcm,bcm4329-fmac";
		reg = <1>;
		interrupt-parent = <&gpx2>;
		interrupts = <0 IRQ_TYPE_EDGE_RISING>;
		interrupt-names = "host-wake";
		pinctrl-names = "default";
		pinctrl-0 = <&wlan_default_mode>;
	};

};

/* uSD card */
&mmc_2 {
	status = "okay";
	cap-sd-highspeed;
	card-detect-delay = <200>;
	clock-frequency = <400000000>;
	samsung,dw-mshc-ciu-div = <3>;
	samsung,dw-mshc-sdr-timing = <2 3>;
	samsung,dw-mshc-ddr-timing = <1 2>;
	pinctrl-names = "default";
	pinctrl-0 = <&sd2_clk &sd2_cmd &sd2_cd &sd2_bus1 &sd2_bus4>;
	bus-width = <4>;

	status = "disabled";
};

&pinctrl_0 {
	wifi_en: wifi-en-pins {
		samsung,pins = "gpy7-7";
		samsung,pin-function = <EXYNOS_PIN_FUNC_OUTPUT>;
		samsung,pin-pud = <EXYNOS_PIN_PULL_NONE>;
		samsung,pin-drv = <EXYNOS5420_PIN_DRV_LV4>;
	};

	lcd_rst: lcd-reset {
		samsung,pins = "gpy7-4";
		samsung,pin-function = <EXYNOS_PIN_FUNC_OUTPUT>;
		samsung,pin-pud = <EXYNOS_PIN_PULL_NONE>;
	};

	mmc2_cd: mmc2-cd-pins {
		samsung,pins = "gpx2-4";
		samsung,pin-pud = <EXYNOS_PIN_PULL_NONE>;
	};

	touch_pin: touch-int-pin {
		samsung,pins = "gpx1-6";
		samsung,pin-pud = <EXYNOS_PIN_PULL_DOWN>;
	};

	wlan_default_mode: wlan-default-mode {
		samsung,pins = "gpx2-0";
		samsung,pin-pud = <EXYNOS_PIN_PULL_DOWN>;
	};

	s2mps11_irq: s2mps11-irq-pins {
		samsung,pins = "gpx0-7";
		samsung,pin-function = <EXYNOS_PIN_FUNC_F>;
		samsung,pin-pud = <EXYNOS_PIN_PULL_NONE>;
		samsung,pin-drv = <EXYNOS5420_PIN_DRV_LV1>;
	};
};

&pinctrl_1 {
	te_irq: te-irq-pins {
		samsung,pins = "gpd1-7";
		samsung,pin-function = <EXYNOS_PIN_FUNC_F>;
	};
};

&rtc {
	status = "okay";
	clocks = <&clock CLK_RTC>, <&s2mps11_osc S2MPS11_CLK_AP>;
	clock-names = "rtc", "rtc_src";
};

&tmu_cpu0 {
	vtmu-supply = <&ldo10_reg>;
};

&tmu_cpu1 {
	vtmu-supply = <&ldo10_reg>;
};

&tmu_cpu2 {
	vtmu-supply = <&ldo10_reg>;
};

&tmu_cpu3 {
	vtmu-supply = <&ldo10_reg>;
};

&tmu_gpu {
	vtmu-supply = <&ldo10_reg>;
};

&usbdrd_dwc3_0 {
	dr_mode = "peripheral";
};

&usbdrd_dwc3_1 {
	dr_mode = "peripheral";
};

&usbdrd3_0 {
	vdd33-supply = <&ldo9_reg>;
	vdd10-supply = <&ldo11_reg>;
};

&usbdrd3_1 {
	vdd33-supply = <&ldo9_reg>;
	vdd10-supply = <&ldo11_reg>;
};

&timer {
	arm,cpu-registers-not-fw-configured;
};

&mixer {
	status = "okay";
};

&fimd {
	status = "okay";
	samsung,invert-vclk;
};

&dsi {
	status = "disabled";

	samsung,pll-clock-frequency = <24000000>;
	samsung,burst-clock-frequency = <512000000>;
	samsung,esc-clock-frequency = <19000000>;

	vddcore-supply = <&ldo8_reg>;
	vddio-supply = <&ldo10_reg>;

	pinctrl-names = "default";
	pinctrl-0 = <&te_irq>;

	te-gpio = <&gpd1 7 GPIO_ACTIVE_HIGH>;

	panel@0 {
		reg = <0>;
		compatible = "samsung,s6e3fa2";

		reset-gpios = <&gpy7 4 GPIO_ACTIVE_LOW>;

		iovdd-supply = <&ldo25_reg>;
		vddr-supply= <&ldo28_reg>;
		pinctrl-names = "default";
		pinctrl-0 = <&lcd_rst>;
	};
};


&i2c_0 {
	status = "okay";

	touchscreen@20 {
		compatible = "syna,rmi4-i2c";
		reg = <0x20>;
		pinctrl-0 = <&touch_pin>;
		pinctrl-names = "default";
		interrupt-parent = <&gpx1>;
		interrupts = <6 IRQ_TYPE_EDGE_FALLING>;
		vio-supply = <&ldo32_reg>;
		vdd-supply = <&tsp_en>;
		syna,startup-delay-ms = <100>;


		#address-cells = <1>;
		#size-cells = <0>;
		rmi4-f01@1 {
			reg = <0x1>;
			syna,nosleep-mode = <1>;
		};

		rmi4-f12@12 {
			reg = <0x12>;
			syna,sensor-type = <1>;
		};
	};

};